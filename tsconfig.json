{
  "compilerOptions": {
    "module": "esNext",
    "target": "es5",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "./dist/",
    "noImplicitThis": true,
    "strictPropertyInitialization": false
  }
}
